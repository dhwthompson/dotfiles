[user]
    name = David Thompson
    email = mail@fatbusinessman.com

[alias]
    st = status -sb
    b = branch
    d = diff
    glog = log --graph --oneline
    one = log --oneline
    co = checkout
    com = checkout master
    fp = fetch --prune
    ff = merge --ff-only
    m = merge --no-ff
    pff = pull --ff-only

[core]
    excludesfile = ~/.gitignore
    quotepath = false

[color]
    ui = true

[push]
    default = upstream

[merge]
    defaultToUpstream = true
